-- Update the refresh token in the database
UPDATE integration_status
SET token = 'value'
WHERE platform='OUTLOOK';
EOF

# Function to parse ini file and extract the value for a given key under a given section
get_ini_value() {
    local file=
    local key=
    awk -F '=' -v key="" ' == key { gsub(/^[ \t]+|[ \t]+$/, "", ); print ; exit }' ""
}

