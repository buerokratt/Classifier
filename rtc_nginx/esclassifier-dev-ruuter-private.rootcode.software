server {
        listen 80;
        listen [::]:80;

        root /var/www/esclassifier-dev-ruuter-private.rootcode.software/html;
        index index.html index.htm index.nginx-debian.html;

        server_name esclassifier-dev-ruuter-private.rootcode.software;

        location / {
                try_files $uri $uri/ =404;
        }
}